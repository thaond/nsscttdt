		<finder name="SignerName" return-type="Collection">
			<finder-column name="signerName" comparator="LIKE"/>
		</finder>
		<finder name="SendDate" return-type="Collection">
			<finder-column name="sendDate" />
		</finder>
		<finder name="DocumentReference" return-type="Collection">
			<finder-column name="documentReference" comparator="LIKE"/>
		</finder>
		<finder name="EditorId" return-type="Collection">
			<finder-column name="editorId" />
		</finder>
		<finder name="SignerId" return-type="Collection">
			<finder-column name="signerId" />
		</finder>
		<finder name="DocumentTypeId" return-type="Collection">
			<finder-column name="documentTypeId" />
		</finder>
		<finder name="PrivilegeLevelId" return-type="Collection">
			<finder-column name="privilegeLevelId" comparator="LIKE"/>
		</finder>
		<finder name="ConfidentialLevelId" return-type="Collection">
			<finder-column name="confidentialLevelId" comparator="LIKE"/>
		</finder>
		<finder name="IssuingDate" return-type="Collection">
			<finder-column name="issuingDate" />
		</finder>
		<finder name="StatusId" return-type="Collection">
			<finder-column name="statusId" comparator="="/>
		</finder>	
		<finder name="IsCongVanPhucDap" return-type="Collection">
			<finder-column name="iscongvanphucdap" comparator="="/>
		</finder>	
		<finder name="OrgExternalId" return-type="Collection">
			<finder-column name="orgExternalId" comparator="="/>
		</finder>	
	</entity>
	
	<entity name="PmlEdmAnswerDetail" local-service="true" remote-service="true"
		table="pml_edm_answerdetail">

		<!-- PK fields -->

		<column name="documentReceiptId" type="long" primary="true" db-name="documentreceiptid"/>

		<!-- Other fields -->
		<column name="documentSendId" type="long" db-name="documentsendid"/>

		<!-- Finder methods -->
		<finder name="DocumentSendId" return-type="Collection">
			<finder-column name="documentSendId" />
		</finder>

	</entity>
	<exceptions>
		<exception>PmlEdmBookDocumentSendCanNotDelete</exception>
		<exception>PmlEdmDocumentSendCanNotPublish</exception>
	</exceptions>
</service-builder>
<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="com.nss.portlet.partner">
	<namespace>Partner</namespace>
	<entity name="Partner" local-service="true" remote-service="true" table="nss_partner">
		
		<column name="maPartner" type="long" db-name="ma_partner" primary="true" />
		
		<column name="tenPartner" type="String" db-name="ten_partner" />
		<column name="urlPartner" type="String" db-name="url_partner" />
		<column name="moTaPartner" type="String" db-name="mo_ta_partner" />
		
		<column name="companyid" type="long" db-name="companyid" />
	  	<column name="userid" type="long" db-name="userid" />
	  	<column name="createdate" type="Date" db-name="createdate" />
	  	<column name="modifieddate" type="Date" db-name="modifieddate" />
		
		<column name="thuTuPartner" type="int" db-name="thu_tu_partner" />
		<column name="target" type="String" db-name="target" />
		<column name="active" type="int" db-name="active" />
		<column name="imageId_liferay" type="long" db-name="imageid_liferay" />
		
		<!-- Order -->
	    <order by="ASC">
	    	<order-column name="thuTuPartner" case-sensitive="false" />
	    </order>
	</entity>
	
</service-builder>